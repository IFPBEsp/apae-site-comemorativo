# Configurações do Banco de Dados PostgreSQL
POSTGRES_DB=apae_db
POSTGRES_USER=apae_user
POSTGRES_PASSWORD=apae_password

# URL de conexão com o banco de dados (usada pela aplicação)
DATABASE_URL=postgresql://apae_user:apae_password@localhost:5432/apae_db

# Configurações do MinIO
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123

# Configurações da aplicação
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# Configurações do MinIO para a aplicação
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET_NAME=apae-files

# Configurações de autenticação (se necessário)
JWT_SECRET=your-jwt-secret-key-here
